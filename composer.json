{
    "name": "ravenna/translate",
    "description": "A Laravel package to translate your application.",
    "authors": [
        {
            "name": "TJ Sherril",
            "email": "tj@ravennainteractive.com"
        },
        {
            "name": "James Hamann",
            "email": "james@ravennainteractive.com"
        },
        {
            "name": "Melody Menzies",
            "email": "melody@ravennainteractive.com"
        },
        {
            "name": "Dominic Sears",
            "email": "dominicsears.work@gmail.com"
        }
    ],
    "license": "GPL-3.0-or-later",
    "type": "library",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/auth": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/container": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^8.12|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.12|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ravenna\\Translate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ravenna\\Translate\\TranslateServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
